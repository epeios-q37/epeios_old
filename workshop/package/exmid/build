# $Id$
mkdir workdir
cd workdir
export DIR=exmid
GET=../../get_cvs

mkdir -p $DIR

$GET epeios $DIR stable/*.h
$GET epeios $DIR stable/*.cpp
$GET emsc $DIR stable/*.h
$GET emsc $DIR stable/*.cpp

$GET epeios $DIR documentation/licenses/gnu_gpl.txt
$GET epeios $DIR documentation/packages/$DIR/README.txt
$GET emsc $DIR apps/$DIR/CHANGELOG.txt
$GET emsc $DIR apps/$DIR/Makefile
$GET emsc $DIR apps/$DIR/exmid.cpp
cp /cygdrive/h/bin/exmid.exe $DIR

mv $DIR/CHANGELOG.txt $DIR/CHANGELOG
mv $DIR/gnu_gpl.txt $DIR/LICENSE
mv $DIR/README.txt $DIR/README

../../create_pkg $DIR
