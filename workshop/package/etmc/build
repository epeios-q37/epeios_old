# $Id$
mkdir workdir
cd workdir
export DIR=etmc-sources
GET=../../get_cvs

$GET epeios $DIR stable/*.h
$GET epeios $DIR stable/*.cpp

$GET epeios $DIR documentation/licenses/gnu_gpl.txt
$GET epeios $DIR documentation/packages/etmc/README.txt

$GET etmc $DIR stable/*.h
$GET etmc $DIR stable/*.cpp

$GET etmc $DIR devel/CHANGELOG.txt

mv $DIR/CHANGELOG.txt $DIR/CHANGELOG
mv $DIR/gnu_gpl.txt $DIR/LICENSE
mv $DIR/README.txt $DIR/README

cp ~/web/etmc/quickdoc/quickdoc.html $DIR

../../create_pkg $DIR