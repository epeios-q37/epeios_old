# $Id$
mkdir workdir
cd workdir
DIR=ecgi
GET=../../get_cvs

mkdir -p $DIR

$GET epeios $DIR stable/*.h
$GET epeios $DIR stable/*.cpp

$GET epeios $DIR documentation/licenses/gnu_gpl.txt
$GET epeios $DIR documentation/packages/ecgi/README.txt

$GET ecgi $DIR stable/*.h
$GET ecgi $DIR stable/*.cpp

$GET ecgi $DIR devel/CHANGELOG.txt

mv $DIR/CHANGELOG.txt $DIR/CHANGELOG
mv $DIR/gnu_gpl.txt $DIR/LICENSE
mv $DIR/README.txt $DIR/README
cp /web/ecgi/quickdoc/quickdoc.html $DIR

../../create_pkg $DIR
