# $Id$
mkdir workdir
cd workdir
GET=../../get_cvs
export DIR=ebkd-sources

$GET epeios $DIR stable/
$GET epeios $DIR documentation/licenses/gnu_gpl.txt
$GET epeios $DIR documentation/packages/ebkd/README.txt

$GET ebkd $DIR stable/
$GET ebkd $DIR devel/CHANGELOG.txt

mv $DIR/CHANGELOG.txt $DIR/CHANGELOG
mv $DIR/gnu_gpl.txt $DIR/LICENSE
mv $DIR/README.txt $DIR/README
cp ~/web/ebkd/quickdoc/quickdoc.html $DIR

../../create_pkg $DIR

