# $Id$
mkdir workdir
cd workdir
export DIR=tmcde-CGID-sources
GET=../../../get_cvs

$GET epeios $DIR stable/*.h
$GET epeios $DIR stable/*.cpp

$GET epeios $DIR documentation/licenses/gnu_gpl.txt
$GET epeios $DIR documentation/packages/tmcde/cgid/README.txt

$GET ecgi $DIR stable/*.h
$GET ecgi $DIR stable/*.cpp


$GET ebkd $DIR stable/*.h
$GET ebkd $DIR stable/*.cpp


$GET exml $DIR stable/*.h
$GET exml $DIR stable/*.cpp


$GET etmc $DIR apps/cgid/*.h
$GET etmc $DIR apps/cgid/*.cpp

$GET etmc $DIR apps/cgid/xsl/*.xsl

mv $DIR/CHANGELOG.txt $DIR/CHANGELOG
mv $DIR/gnu_gpl.txt $DIR/LICENSE
mv $DIR/README.txt $DIR/README
rm $DIR/bkd4cpp.xsl

../../../create_pkg $DIR