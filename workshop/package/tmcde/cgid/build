# $Id$
mkdir workdir
cd workdir
export DIR=tmcde-CGID-sources
GET=../../../get_cvs

$GET epeios $DIR stable/
$GET epeios $DIR documentation/licenses/gnu_gpl.txt
$GET epeios $DIR documentation/packages/tmcde/cgid/README.txt

$GET ecgi $DIR stable/

$GET ebkd $DIR stable/

$GET exml $DIR stable/

$GET etmc $DIR apps/cgid/
$GET etmc $DIR apps/cgid/xsl/

mv $DIR/CHANGELOG.txt $DIR/CHANGELOG
mv $DIR/gnu_gpl.txt $DIR/LICENSE
