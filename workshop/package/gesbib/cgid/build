# $Id$
mkdir workdir
cd workdir
export DIR=gesbib-cgid-sources
mkdir $DIR
GET=../../../get_cvs

$GET epeios $DIR stable/*.h
$GET epeios $DIR stable/*.cpp

$GET ecgi $DIR stable/*.h
$GET ecgi $DIR stable/*.cpp

$GET exml $DIR stable/*.h
$GET exml $DIR stable/*.cpp

$GET ebkd $DIR stable/*.h
$GET ebkd $DIR stable/*.cpp

$GET lgsb $DIR apps/gesbib/frontend/*.h
$GET lgsb $DIR apps/gesbib/frontend/*.cpp
$GET lgsb $DIR apps/gesbib/frontend/cgid/*.h
$GET lgsb $DIR apps/gesbib/frontend/cgid/*.cpp
$GET lgsb $DIR apps/gesbib/frontend/cgid/Makefile

#../../../create_pkg $DIR
tar --gzip -cf $DIR.tar.gz $DIR

