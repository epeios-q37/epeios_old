# $Id$
mkdir workdir
cd workdir
export DIR=emth-sources
GET=../../get_cvs

$GET epeios $DIR stable/
$GET epeios $DIR documentation/licenses/gnu_gpl.txt
$GET epeios $DIR documentation/packages/emth/README.txt

$GET emth $DIR stable/
$GET emth $DIR devel/CHANGELOG.txt

mv $DIR/CHANGELOG.txt $DIR/CHANGELOG
mv $DIR/gnu_gpl.txt $DIR/LICENSE
mv $DIR/README.txt $DIR/README
cp ~/web/emth/quickdoc/quickdoc.html $DIR

../../create_pkg $DIR